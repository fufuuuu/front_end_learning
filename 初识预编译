// 1.创建AO对象（Activation Object）
// 2.找形参和变量声明，将变量和形参名作AO属性名，值为undefined
// 3.将实参值和形参统一
// 4.在函数体里面找出函数声明，值赋予函数体
 function fn(a) {
 	console.log(a);

 	var a = 123;

 	console.log(a);

 	function a() {

 	}
 	console.log(a);
 	var b = function(){

 	}
 	console.log(b);
 	function d() {

 	}
}
fn(1);

//输出结果为f  a; 123 ; 123 ; f
